{
  "name": "@tmplr/node",
  "description": "Node.js bindings for tmplr",
  "version": "0.2.2",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist/commonjs",
    "dist/es"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build-commonjs": "tsc -p conf/typescript/commonjs.json",
    "build-es": "tsc -p conf/typescript/es.json",
    "build": "npm run build-commonjs && npm run build-es",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loreanvictor/tmplr-node.git"
  },
  "author": "Eugene Ghanizadeh Khoub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loreanvictor/tmplr-node/issues"
  },
  "homepage": "https://github.com/loreanvictor/tmplr-node#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/degit": "^2.8.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-jest": "^28.1.1",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@tmplr/core": "^0.7.0",
    "git-url-parse": "^12.0.0",
    "simple-git": "^3.7.1",
    "tiged": "^2.12.7"
  }
}
